{
  "uid": "boz-kalshi-ws-feed",
  "title": "Kalshi WS Feed",
  "description": "Kalshi WebSocket market data feed connection status, message throughput, cache hit rates, and reconnection health.",
  "tags": ["boz", "kalshi", "websocket"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "templating": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "title": "Feed Connection Status",
      "description": "Kalshi WebSocket feed connection status. 1 = connected, 0 = disconnected.",
      "type": "stat",
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          },
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": { "text": "Disconnected", "index": 0 },
                "1": { "text": "Connected", "index": 1 }
              }
            }
          ]
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "kalshi_ws_connected",
          "legendFormat": "Status",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"]
        },
        "colorMode": "background"
      }
    },
    {
      "id": 2,
      "title": "Reconnections (Last Hour)",
      "description": "Number of WebSocket reconnection attempts in the last hour.",
      "type": "stat",
      "gridPos": { "h": 8, "w": 6, "x": 6, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 3 }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "increase(kalshi_ws_reconnects_total[1h])",
          "legendFormat": "Reconnects",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"]
        },
        "colorMode": "background"
      }
    },
    {
      "id": 3,
      "title": "Cache Hit Rate",
      "description": "Percentage of price lookups served from the WebSocket cache versus REST API fallback.",
      "type": "gauge",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 50 },
              { "color": "green", "value": 90 }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(rate(kalshi_ws_cache_hits_total{source=\"cache\"}[5m])) / sum(rate(kalshi_ws_cache_hits_total[5m])) * 100",
          "legendFormat": "Hit Rate",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"]
        }
      }
    },
    {
      "id": 4,
      "title": "Message Throughput by Channel",
      "description": "WebSocket message rate by channel (ticker, orderbook_delta, other).",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20,
            "stacking": { "mode": "normal" }
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(rate(kalshi_ws_messages_total[5m])) by (channel)",
          "legendFormat": "{{channel}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 5,
      "title": "Reconnection Events",
      "description": "WebSocket reconnection attempts over time. Spikes indicate connection instability.",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "increase(kalshi_ws_reconnects_total[5m])",
          "legendFormat": "Reconnects",
          "refId": "A"
        }
      ]
    },
    {
      "id": 6,
      "title": "Cache Source Distribution",
      "description": "Distribution of price lookups between WebSocket cache and REST API fallback over the last hour.",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(increase(kalshi_ws_cache_hits_total[1h])) by (source)",
          "legendFormat": "{{source}}",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"]
        },
        "pieType": "donut",
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": ["value", "percent"]
        }
      }
    }
  ]
}
