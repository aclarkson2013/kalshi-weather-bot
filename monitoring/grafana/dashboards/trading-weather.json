{
  "uid": "boz-trading-weather",
  "title": "Trading & Weather",
  "description": "Trading cycle outcomes, trade execution, risk blocks, weather fetch success rates, and Celery task performance.",
  "tags": ["boz", "trading", "weather", "celery"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "templating": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "title": "Trading Cycle Outcomes",
      "description": "Trading cycle completions, skips, timeouts, and errors over time.",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20,
            "stacking": { "mode": "normal" }
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "completed" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "skipped" },
            "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "error" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "timeout" },
            "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }]
          }
        ]
      },
      "targets": [
        {
          "expr": "sum(rate(trading_cycles_total[5m])) by (outcome)",
          "legendFormat": "{{outcome}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 2,
      "title": "Trading Cycle Success Rate",
      "description": "Percentage of trading cycles that completed successfully.",
      "type": "stat",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 50 },
              { "color": "green", "value": 80 }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(rate(trading_cycles_total{outcome=\"completed\"}[5m])) / sum(rate(trading_cycles_total[5m])) * 100",
          "legendFormat": "Success %",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"]
        },
        "colorMode": "background"
      }
    },
    {
      "id": 3,
      "title": "Trades Executed Total",
      "description": "Cumulative count of trades executed or queued.",
      "type": "stat",
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(trades_executed_total)",
          "legendFormat": "Total Trades",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"]
        },
        "colorMode": "background"
      }
    },
    {
      "id": 4,
      "title": "Trades Executed by City & Mode",
      "description": "Trade execution rate broken down by city and execution mode (auto vs queued).",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 80,
            "stacking": { "mode": "normal" }
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(rate(trades_executed_total[5m])) by (city, mode)",
          "legendFormat": "{{city}} ({{mode}})",
          "refId": "A"
        }
      ]
    },
    {
      "id": 5,
      "title": "Trades Risk Blocked",
      "description": "Trades blocked by the risk manager, grouped by reason.",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(rate(trades_risk_blocked_total[5m])) by (reason)",
          "legendFormat": "{{reason}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 6,
      "title": "Weather Fetch Success Rate",
      "description": "Percentage of successful weather fetches by data source.",
      "type": "stat",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 80 },
              { "color": "green", "value": 95 }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(rate(weather_fetches_total{outcome=\"success\"}[5m])) by (source) / sum(rate(weather_fetches_total[5m])) by (source) * 100",
          "legendFormat": "{{source}}",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"]
        },
        "colorMode": "background",
        "graphMode": "area"
      }
    },
    {
      "id": 7,
      "title": "Weather Fetches by Source",
      "description": "Weather data fetch rate broken down by source (NWS, Open-Meteo, etc).",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 15
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(rate(weather_fetches_total[5m])) by (source, outcome)",
          "legendFormat": "{{source}} ({{outcome}})",
          "refId": "A"
        }
      ]
    },
    {
      "id": 8,
      "title": "Weather Fetches by City",
      "description": "Weather fetch volume per city over the selected time range.",
      "type": "barchart",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 24 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(increase(weather_fetches_total[1h])) by (city)",
          "legendFormat": "{{city}}",
          "refId": "A",
          "format": "table",
          "instant": true
        }
      ],
      "options": {
        "orientation": "horizontal",
        "stacking": "none",
        "showValue": "always"
      }
    },
    {
      "id": 9,
      "title": "Celery Task Rate",
      "description": "Celery task execution rate by task name and status (success, failure, retry).",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 24 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 15
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(rate(celery_task_total[5m])) by (task_name, status)",
          "legendFormat": "{{task_name}} ({{status}})",
          "refId": "A"
        }
      ]
    },
    {
      "id": 10,
      "title": "Celery Task Duration P95",
      "description": "95th percentile task duration by task name â€” find slow tasks.",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 24 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(celery_task_duration_seconds_bucket[5m])) by (le, task_name))",
          "legendFormat": "{{task_name}}",
          "refId": "A"
        }
      ]
    }
  ]
}
